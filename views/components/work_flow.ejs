                    <div class="w-1/4 text-center">type</div>
                    <div class="w-1/4 text-center">progress</div>
                </div>
            </div>


            
            <% for( let i = 0; i < workspaces.length; i++ ) { %>
                <% for( let k = 0; k < workspaces[i].lists.length; k++ ) { %>
